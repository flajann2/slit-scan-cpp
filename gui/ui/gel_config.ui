<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.92.0 -->
<interface>
  <!-- interface-name gel_config.ui -->
  <!-- interface-description Configuration of individual gels. -->
  <requires lib="gtk" version="4.14"/>
  <object class="GtkDialog" id="ss_gel_config">
    <property name="default-height">600</property>
    <property name="default-width">800</property>
    <property name="title">Gel Config</property>
    <child>
      <object class="GtkNotebook" id="ss_gel_config_note">
        <property name="group-name">ss_gel_config_note</property>
        <property name="hexpand">True</property>
        <property name="page">3</property>
        <property name="tab-pos">left</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkGrid" id="ss_gel_config_note_p1">
            <property name="column-spacing">2</property>
            <property name="row-spacing">2</property>
            <child>
              <object class="GtkDrawingArea" id="ss_gel_config_p1_canvas">
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">4</property>
                  <property name="row">0</property>
                  <property name="row-span">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="ss_gel_config_p1_rate">
                <property name="adjustment">
                  <object class="GtkAdjustment" id="ss_gel_config_p1_rate_adj">
                    <property name="step-increment">10.0</property>
                    <property name="upper">100.0</property>
                    <property name="value">50.0</property>
                  </object>
                </property>
                <property name="fill-level">100.0</property>
                <property name="hexpand">True</property>
                <property name="show-fill-level">True</property>
                <property name="valign">center</property>
                <layout>
                  <property name="column">5</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="justify">right</property>
                <property name="label">Rotate</property>
                <layout>
                  <property name="column">4</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="justify">right</property>
                <property name="label">Rate</property>
                <layout>
                  <property name="column">4</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="justify">right</property>
                <property name="label">Flip</property>
                <layout>
                  <property name="column">4</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="ss_gel_config_p1_rotate">
                <property name="adjustment">
                  <object class="GtkAdjustment" id="ss_gel_config_p1_rotate_adj">
                    <property name="step-increment">10.0</property>
                    <property name="upper">100.0</property>
                    <property name="value">50.0</property>
                  </object>
                </property>
                <property name="fill-level">100.0</property>
                <property name="hexpand">True</property>
                <property name="show-fill-level">True</property>
                <property name="valign">center</property>
                <layout>
                  <property name="column">5</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch">
                <layout>
                  <property name="column">5</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSwitch" id="ss_gel_config_p1_flip">
                <property name="hexpand">True</property>
                <property name="valign">center</property>
                <layout>
                  <property name="column">5</property>
                  <property name="column-span">1</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Image</property>
          </object>
        </child>
        <child>
          <object class="GtkGrid" id="ss_gel_config_note_p2">
            <child>
              <object class="GtkDrawingArea" id="ss_gel_config_p2_canvas">
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">4</property>
                  <property name="row">0</property>
                  <property name="row-span">4</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Camera</property>
          </object>
        </child>
        <child>
          <object class="GtkGrid" id="ss_gel_config_note_p3">
            <child>
              <object class="GtkDrawingArea" id="ss_gel_config_p3_canvas">
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">4</property>
                  <property name="row">0</property>
                  <property name="row-span">4</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel">
            <property name="label">Video</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
